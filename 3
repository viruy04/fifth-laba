#include <stdio.h>
int main() {
    int a;
    short int b[5]={82,5,-6,-1,31};
    for (int i=0; i!=5; i++) {
        for (int c=0; c!=4; c++) {
            if (b[c]>b[c+1]) 
            {
                a=b[c];
                b[c]=b[c+1];
                b[c+1]=a;
            };
        };
    }
    for (int i=0; i!=5; i++ ) {        
        printf("%d ", b[i]);
    }
    return 0;
}
